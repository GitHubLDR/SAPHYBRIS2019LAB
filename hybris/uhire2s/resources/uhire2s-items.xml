	<collectiontypes>
		<collectiontype elementtype="java.lang.String" code="hobbiesStringCollection"
			type="set" />

	</collectiontypes>
	<enumtypes>
		<enumtype code="StudentType" autocreate="true" generate="true"
			dynamic="true">
			<value code="Regular"></value>
			<value code="Distance"></value>

		</enumtype>
		<enumtype code="OrderStatus" autocreate="false" generate="true"
			dynamic="true">
			<value code="MANUAL_CODE"></value>
			<value code="ORDER_PACKED"></value>
		</enumtype>
	</enumtypes>
	<maptypes>
		<maptype argumenttype="java.util.Date" returntype="java.lang.String"
			code="feedback" autocreate="true" />
	</maptypes>
	<relations>
		<relation localized="true" code="Student2CourseRel"
			autocreate="true" generate="true">
			<deployment table="student2courserel" typecode="12345" />
			<sourceElement type="Student" collectiontype="set"
				cardinality="many" ordered="true" qualifier="student" />
			<targetElement type="Course" collectiontype="set"
				cardinality="many" ordered="false" qualifier="course" />
		</relation>

		<relation code="CategoryProductRelation" autocreate="true"
			generate="true" localized="false">
			<deployment table="Cat2ProdRel" typecode="143" />
			<sourceElement qualifier="supercategories" type="Category"
				cardinality="many" ordered="false">
				<description>Super Categories</description>
				<modifiers read="true" write="true" search="true"
					optional="true" />
			</sourceElement>
			<targetElement qualifier="products" type="Product"
				cardinality="many" collectiontype="list" ordered="true">
				<description>Products</description>
				<modifiers read="true" write="true" search="true"
					optional="true" />
			</targetElement>
		</relation>
		<relation localized="false" code="Student2LibraryCardRel"
			autocreate="true" generate="true">
			<sourceElement type="Student" cardinality=" one"
				qualifier="student" ordered="false"></sourceElement>
			<targetElement type="LibraryCard" collectiontype="set"
				cardinality="many" qualifier="librarycards" ordered="true"></targetElement>


		</relation>



	</relations>

	<itemtypes>
		<itemtype code="Student" autocreate="true" generate="true">
			<deployment table="student" typecode="10001" />

			<attributes>
				<attribute type="java.lang.Boolean" qualifier="id">
					<persistence type="property"></persistence>
				</attribute>
				<attribute type="java.lang.String" qualifier="name">
					<persistence type="property"></persistence>
				</attribute>
				<!-- <attribute type="java.lang.String" qualifier="course"> <persistence 
					type="property"></persistence> </attribute> -->
				<attribute type="StudentType" qualifier="studentType">
					<persistence type="property"></persistence>
				</attribute>
				<attribute type="OrderStatus" qualifier="orderStatus">
					<persistence type="property"></persistence>
				</attribute>

				<attribute type="hobbiesStringCollection" qualifier="hobbies">
					<persistence type="property"></persistence>
				</attribute>

			</attributes>
		</itemtype>
		<itemtype code="Course" autocreate="true" generate="true">
			<deployment table="course" typecode="10008" />
			<attributes>
				<attribute type="java.lang.String" qualifier="java">
					<persistence type="property"></persistence>
				</attribute>
				<attribute type="java.lang.String" qualifier="hybris">
					<persistence type="property"></persistence>
				</attribute>
			</attributes>
		</itemtype>
		<itemtype code="LibraryCard">
			<deployment table="libraryCard" typecode="10212" />
			<attributes>
				<attribute type="java.lang.String" qualifier="uid">
					<persistence type="property"></persistence>
				</attribute>
				<attribute type="java.util.Date" qualifier="dateofIssue">
					<persistence type="property"></persistence>
				</attribute>
				<attribute type="feedback" qualifier="feedback">
					<persistence type="property"></persistence>
				</attribute>

			</attributes>
		</itemtype>

	</itemtypes>

</items>
